{
	"flavor": "nlb_nginx",
	"metadata": {
		"name": "main",
		"annotations": {
			"nginx.ingress.kubernetes.io/use-regex": "true"
		}
	},
	"advanced": {
		"gcp_alb": {
			"enable_managed_certificates": false
		}
	},
	"kind": "ingress",
	"disabled": false,
	"version": "0.1",
	"spec": {
		"basicAuth": false,
		"private": false,
		"force_ssl_redirection": true,
		"rules": {
			"frontend": {
				"comment": "front-end",
				"domain_prefix": "",
				"path": "/",
				"port": "${service.new-test-service.out.interfaces.main.port}",
				"service_name": "${service.new-test-service.out.interfaces.main.name}"
			},
			"backend": {
				"comment": "back-end",
				"domain_prefix": "",
				"path": "/api/employees/",
				"port": "${service.be.out.interfaces.http.port}",
				"service_name": "${service.be.out.interfaces.http.name}"
			}
		}
	}
}
